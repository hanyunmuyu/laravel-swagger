{
    "name": "hanyun/laravel-swagger",
    "description": "Autogenerate OpenAPI for laravel with Swagger-UI",
    "keywords": [
        "swagger",
        "laravel",
        "openapi"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "hanyun",
            "email": "1355081829@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hanyun\\Swagger\\": "src/"
        }
    },
    "require": {
        "php": "^7.3",
        "zircote/swagger-php": "^3.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hanyun\\Swagger\\SwaggerProvider"
            ],
            "aliases": {
                "Swagger": "Hanyun\\Swagger\\Facades\\Swagger"
            }
        }
    }
}
